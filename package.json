{
  "name": "mcp-browser",
  "version": "1.2.0",
  "description": "AI-powered browser that queries MCP servers and renders results as HTML",
  "author": "MCP Browser",
  "main": "out/main/index.js",
  "scripts": {
    "dev": "electron-vite dev",
    "build": "electron-vite build && node scripts/build-servers.mjs",
    "preview": "electron-vite preview",
    "package": "electron-vite build && node scripts/build-servers.mjs && electron-builder",
    "typecheck": "tsc --noEmit -p tsconfig.node.json && tsc --noEmit -p tsconfig.web.json"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.78.0",
    "@modelcontextprotocol/sdk": "^1.12.0",
    "duckduckgo-mcp-server": "^0.1.2",
    "electron-store": "^10.0.0",
    "mcp-fetch": "^0.1.6",
    "react": "^18.3.0",
    "react-dom": "^18.3.0"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "electron",
      "esbuild",
      "electron-builder"
    ]
  },
  "devDependencies": {
    "@types/node": "^22",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.0",
    "electron": "^35.0.0",
    "electron-builder": "^26.8.1",
    "electron-vite": "^3.0.0",
    "typescript": "^5.5.0",
    "vite": "^6.0.0"
  }
}
